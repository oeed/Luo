local n = 100000000
local start1 = os.clock()
one="Hello there!"
for i = 1, n do
	local two = one .. ": this"
end
local total1 = os.clock() - start1

local start2 = os.clock()
local three="Hello there!"
for i = 1, n do
	local two = three .. ": this"
end
local total2 = os.clock() - start2

print("Total 1:" .. total1)
print("Average 1:" .. total1 / n)
print("Total 2:" .. total2)
print("Average 2:" .. total2 / n)
print(total2/total1)